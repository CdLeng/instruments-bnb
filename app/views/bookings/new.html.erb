<div class="container my-5">
  <h1>Book the instrument</h1>
  <div class="row col-4">
    <%= simple_form_for @booking do |f| %>
      <%= f.input :date_start, as: :date, html5: true %>
      <%= f.input :date_end, as: :date, html5: true %>
      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
